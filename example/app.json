{
  "expo": {
    "name": "balloon-slider-example",
    "description": "Example app for react-native-balloon-slider",
    "slug": "balloon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "sdkVersion": "38.0.0",
    "platforms": ["android", "ios", "web"],
    "githubUrl": "https://github.com/osamaq/react-native-balloon-slider"
  }
}
